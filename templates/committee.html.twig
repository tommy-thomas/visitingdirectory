<head>
	<meta charset="utf-8">
	<title>Secure Directory | Visiting Committees &amp; Councils | The University of Chicago</title>
	
	{{ include('inc/head.html.twig') }}
</head>  
<body>
	{{ include('inc/header.html.twig') }}
	<div class="container">
		{% if $authenticationError %}
			<div class="error">{{ authentication_error }}</div>
		{% endif %}	
		<h1 class="page-title">{{ Committee }}</h1>
		<div class="row">
			<div class="span9 pull-right">
				<div class="maincontent">
					
					<h2>{{ Chairman }}</h2>
					<p class="small"><em>*Denotes a Life Member</em></p>
					<ul class="list-3col">
						{% for m in members_list %}
							<li>
								{{ m.first_name }} {{ m.middle }} {{ m.last_name }}
							</li>		 
						{% endfor %}
					</ul>
				</div>
			</div>
			
			<div class="span3">				
				{{ include('inc/navigation-public.html.twig') }} <!-- this now includes the related links as well -->
				{{ include('inc/related-links.html.twig') }}
			</div>					
					
		</div>
	</div><!-- /#page-wrapper -->
	<div class="push"><!--//--></div>

	{{ include('inc/footer.html.twig') }}

<!-- Insert all js calls here  (jquery is included in the footer inc) -->

</body>
</html>
	