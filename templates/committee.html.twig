{% extends "layout.twig" %}

{% block title %}Secure Directory{% endblock %}

{% block content %}

	<div class="container">
		<h1 class="page-title">{{ Committee }}</h1>
		<div class="row">
			<div class="span9 pull-right">
				<div class="maincontent">
					
					<h2>{{ Chairman }}</h2>
					<p class="small"><em>*Denotes a Life Member</em></p>
					<ul class="list-3col">
						{% for m in members_list %}
							<li>
								{{ m.first_name }} {{ m.middle }} {{ m.last_name }}
							</li>		 
						{% endfor %}
					</ul>
				</div>
			</div>
			
			<div class="span3">				
				{{ include('inc/navigation-public.html.twig') }} <!-- this now includes the related links as well -->
				{{ include('inc/related-links.html.twig') }}
			</div>					
					
		</div>
	</div><
	
{% endblock content %}